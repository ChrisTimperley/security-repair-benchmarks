#!/bin/bash
set -eu

HERE_DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
PATH_EXPLOIT="${HERE_DIR}/exploit"
PATH_LINUX_BUILD="$(find "${HERE_DIR}/source" -name "Linux*.d" -type d)"
PATH_BINS="${PATH_LINUX_BUILD}/bins"
PATH_BIN="${1:-${PATH_BINS}/unzzipcat-mem}"

${TEST_EXECUTION_PREFIX:-} "${PATH_BIN}" "${PATH_EXPLOIT}"
